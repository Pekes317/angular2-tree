  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "build": "npm run build:inline && npm run build:esm2015 && npm run build:fesm2015",
    "build:inline": "rm -rf ./tmp && rsync -a --exclude *spec.ts *.ts  ./tmp & node inlineAssets.js",
    "build:esm2015": "ngc -p tsconfig-es2015.json",
    "build:fesm2015": "rollup -c rollup-esm2015.conf.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "./node_modules/.bin/karma --config karma.conf.js start",
    "gh-pages": "ng build --env=prod --base-href \"https://qjon.github.io/angular2-tree/\" &&  ./node_modules/.bin/angular-cli-ghpages -b gh-pages  --name=\"Rafa≈Ç Ignaszewski\" --email=\"rafal@ignaszewski.pl\" --no-silent"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/qjon/angular2-tree.git"
  },
  "main": "main",
  "author": "Rafal Ignaszewski <rafal@ignaszewski.pl> http://ignaszewski.pl",
  "dependencies": {
    "@angular/animations": "^5.0.0",
