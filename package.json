  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "cpr dist/ngx-tree src/@beezleeart/lib && ng build --app packages && rimraf src/@beezleeart",
    "build:lib": "rimraf dist && ng build ngx-tree-lib && npm run readme",
    "build:prod": "rimraf dist && ng build ngx-tree-lib --prod && npm run readme",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "readme": "copyfiles ./package.README.md ./dist/ngx-tree/ && cd dist/ngx-tree && move package.README.md README.md",
    "publish": "npm publish ./dist/ngx-tree"
  },
  "dependencies": {
