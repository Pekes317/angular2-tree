    "ng-packagr": "^3.0.0-rc.4",
    "ng2-inline-template": "^0.1.1",
    "node-sass": "^4.9.0",
    "protractor": "~5.3.2",
    "renamer": "0.6.1",
    "rxjs-es": "^5.0.0-beta.12",
    "ts-node": "~6.0.3",
