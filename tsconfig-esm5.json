{
  "extends": "./tsconfig-es2015.json",
  "compilerOptions": {
    "target": "es5",
    "outDir": "./tmp/esm5",
    "rootDir": "./tmp"
  },
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "angular2-tree.js",
    "flatModuleId": "angular2-tree"
  }
}
